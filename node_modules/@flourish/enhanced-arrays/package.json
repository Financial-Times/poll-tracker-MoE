{
  "name": "@flourish/enhanced-arrays",
  "version": "1.1.1",
  "description": "Create immutable array-like-objects with useful methods",
  "main": "enhanced-arrays.js",
  "module": "src/index.js",
  "author": "Flourish team",
  "license": "https://flourish.studio/terms/fcl/",
  "scripts": {
    "lint": "eslint src",
    "build": "rollup -c",
    "prepare": "npm run build && npm run minify",
    "minify": "uglifyjs -m -o enhanced-arrays.min.js enhanced-arrays.js",
    "precommit": "lint-staged",
    "pretest": "npm run build",
    "test": "mocha"
  },
  "lint-staged": {
    "*.js": "npm run lint --"
  },
  "dependencies": {
    "d3-array": "1.2.4"
  },
  "devDependencies": {
    "@flourish/eslint-plugin-flourish": "^0.7.2",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mocha": "^6.2.2",
    "rollup": "^1.25.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-js": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
