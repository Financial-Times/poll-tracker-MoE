{
  "name": "@flourish/info-popup",
  "version": "4.0.4",
  "description": "Popup with data insertion",
  "main": "info-popup.js",
  "module": "src/index.js",
  "scripts": {
    "lint": "eslint src",
    "build": "rollup -c",
    "prepare": "npm run build && npm run minify",
    "minify": "uglifyjs -m -o info-popup.min.js info-popup.js",
    "precommit": "lint-staged",
    "watch": "npm-run-all -p watch:*",
    "watch:bundle": "rollup -cw --no-treeshake --no-indent",
    "cypress": "npm-run-all -p watch cypress:*",
    "cypress:open": "flcypress open"
  },
  "author": "Kiln Enterprises Ltd",
  "license": "UNLICENSED",
  "devDependencies": {
    "@flourish/cypress": "^2.12.3",
    "@flourish/eslint-plugin-flourish": "^0.7.2",
    "cypress": "^7.7.0",
    "eslint": "^5.2.0",
    "eslint-plugin-cypress": "^2.11.3",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.53.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "uglify-js": "^2.7.5"
  },
  "dependencies": {
    "@flourish/js2css": "^1.0.0",
    "@flourish/popup": "^2.1.5",
    "d3-color": "^1.4.1",
    "d3-format": "^1.3.2",
    "d3-selection": "^1.4.1",
    "d3-timer": "^1.0.10",
    "d3-transition": "^1.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kiln/flourish-info-popup.git"
  },
  "lint-staged": {
    "*.js": "eslint src"
  }
}
